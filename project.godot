resources_spreadsheet_view/fold_docks=true
resources_spreadsheet_view/resource_cell_label_mode=1

[application]

config/name="stereobleedingedge"
run/main_scene="uid://jyt6sodbakhs"
config/features=PackedStringArray("4.4", "Forward Plus")
boot_splash/bg_color=Color(0.00820202, 0.00820203, 0.00820202, 1)
boot_splash/show_image=false
config/icon="res://icon.svg"

[autoload]

PhantomCameraManager="*res://addons/phantom_camera/scripts/managers/phantom_camera_manager.gd"
signals="*res://old stuff/signalarity.gd"
InitiativeManager="*res://old stuff/combat/initiative_manager.gd"
Dice="*res://addons/dice_engine/dice_engine.gd"

[editor]

version_control/plugin_name="GitPlugin"
version_control/autoload_on_startup=true

[editor_plugins]

